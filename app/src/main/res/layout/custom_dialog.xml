<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/homecard">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline63"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <View
        android:id="@+id/view25"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="-55dp" />

    <View
        android:id="@+id/view26"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        app:layout_constraintTop_toBottomOf="@+id/imgDiCam" />

    <View
        android:id="@+id/view27"
        android:layout_width="@dimen/dp_120"
        android:layout_height="@dimen/dp_50"
        app:layout_constraintBottom_toTopOf="@+id/view26"
        app:layout_constraintEnd_toStartOf="@+id/guideline63"
        app:layout_constraintStart_toStartOf="@+id/guideline63"
        app:layout_constraintTop_toBottomOf="@+id/view25" />

    <View
        android:id="@+id/view28"
        android:layout_width="@dimen/dp_50"
        android:layout_height="@dimen/dp_50"
        app:layout_constraintBottom_toTopOf="@+id/view26"
        app:layout_constraintEnd_toStartOf="@+id/imgDiCam"
        app:layout_constraintTop_toTopOf="@+id/imgDiCam" />

    <View
        android:id="@+id/view29"
        android:layout_width="@dimen/dp_50"
        android:layout_height="@dimen/dp_50"
        app:layout_constraintBottom_toTopOf="@+id/view26"
        app:layout_constraintStart_toEndOf="@+id/imgDiGall"
        app:layout_constraintTop_toTopOf="@+id/imgDiGall" />

    <ImageView
        android:id="@+id/imgDiCam"
        android:layout_width="@dimen/dp_70"
        android:layout_height="@dimen/dp_70"
        app:layout_constraintEnd_toStartOf="@+id/view27"
        app:layout_constraintTop_toBottomOf="@+id/view25"
        app:srcCompat="@drawable/ic_camera"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imgDiGall"
        android:layout_width="@dimen/dp_70"
        android:layout_height="@dimen/dp_70"
        app:layout_constraintStart_toEndOf="@+id/view27"
        app:layout_constraintTop_toBottomOf="@+id/view25"
        app:srcCompat="@drawable/gallareys"
        tools:ignore="ContentDescription" />
</androidx.constraintlayout.widget.ConstraintLayout>