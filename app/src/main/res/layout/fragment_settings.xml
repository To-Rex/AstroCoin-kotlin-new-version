<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/fon"
    tools:context=".ui.notifications.SettingsFragment">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
       <androidx.constraintlayout.widget.ConstraintLayout
           android:layout_width="match_parent"
           android:layout_height="match_parent">
           <androidx.constraintlayout.widget.Guideline
               android:id="@+id/guideline39"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:orientation="vertical"
               app:layout_constraintGuide_percent="0.05" />

           <androidx.constraintlayout.widget.Guideline
               android:id="@+id/guideline40"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:orientation="vertical"
               app:layout_constraintGuide_percent="0.95" />

           <androidx.constraintlayout.widget.Guideline
               android:id="@+id/guideline41"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:orientation="horizontal"
               app:layout_constraintGuide_percent="0.05" />

           <com.google.android.material.imageview.ShapeableImageView
               android:id="@+id/usimage"
               android:layout_width="@dimen/dp_110"
               android:layout_height="@dimen/dp_110"
               android:padding="3dp"
               android:scaleType="centerCrop"
               app:layout_constraintEnd_toStartOf="@+id/guideline40"
               app:layout_constraintHorizontal_bias="0.498"
               app:layout_constraintStart_toStartOf="@+id/guideline39"
               app:layout_constraintTop_toTopOf="@+id/guideline41"
               app:shapeAppearanceOverlay="@style/ShapeAppearanceOverlay.App.CornerSize50Percent"
               app:srcCompat="@drawable/usericons" />

           <View
               android:id="@+id/view5"
               android:layout_width="@dimen/dp_35"
               android:layout_height="@dimen/dp_35"
               android:layout_marginEnd="@dimen/dp_3"
               android:layout_marginBottom="@dimen/dp_2"
               android:background="@drawable/shapeback"
               app:layout_constraintBottom_toBottomOf="@+id/usimage"
               app:layout_constraintEnd_toEndOf="@+id/usimage" />

           <ImageView
               android:id="@+id/imageView10"
               android:layout_width="@dimen/dp_25"
               android:layout_height="@dimen/dp_25"
               android:src="@drawable/ic_camera"
               app:layout_constraintBottom_toBottomOf="@+id/view5"
               app:layout_constraintEnd_toEndOf="@+id/view5"
               app:layout_constraintStart_toStartOf="@+id/view5"
               app:layout_constraintTop_toTopOf="@+id/view5"
               tools:ignore="ContentDescription" />

           <TextView
               android:id="@+id/txtsetfullname"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_marginTop="@dimen/dp_11"
               android:text="@string/connect"
               android:textColor="@color/black"
               android:textSize="20sp"
               app:layout_constraintEnd_toStartOf="@+id/guideline40"
               app:layout_constraintStart_toStartOf="@+id/guideline39"
               app:layout_constraintTop_toBottomOf="@+id/usimage" />

           <TextView
               android:id="@+id/txtsetemail"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_marginTop="@dimen/dp_10"
               android:text="@string/connect"
               app:layout_constraintEnd_toStartOf="@+id/guideline40"
               app:layout_constraintStart_toStartOf="@+id/guideline39"
               app:layout_constraintTop_toBottomOf="@+id/txtsetfullname" />

           <View
               android:id="@+id/view11"
               android:layout_width="0dp"
               android:layout_height="@dimen/dp_45"
               android:layout_marginTop="@dimen/dp_15"
               android:background="@drawable/homecard"
               app:layout_constraintEnd_toStartOf="@+id/guideline40"
               app:layout_constraintStart_toStartOf="@+id/guideline39"
               app:layout_constraintTop_toBottomOf="@+id/txtsetemail" />

           <View
               android:id="@+id/view9"
               android:layout_width="0dp"
               android:layout_height="@dimen/dp_45"
               android:layout_marginTop="@dimen/dp_8"
               android:background="@drawable/homecard"
               app:layout_constraintEnd_toStartOf="@+id/guideline40"
               app:layout_constraintHorizontal_bias="1.0"
               app:layout_constraintStart_toStartOf="@+id/guideline39"
               app:layout_constraintTop_toBottomOf="@+id/view11" />

           <View
               android:id="@+id/view12"
               android:layout_width="0dp"
               android:layout_height="@dimen/dp_141"
               android:layout_marginTop="@dimen/dp_8"
               android:background="@drawable/homecard"
               app:layout_constraintEnd_toStartOf="@+id/guideline40"
               app:layout_constraintHorizontal_bias="0.0"
               app:layout_constraintStart_toStartOf="@+id/guideline39"
               app:layout_constraintTop_toBottomOf="@+id/view9" />

           <View
               android:id="@+id/view13"
               android:layout_width="0dp"
               android:layout_height="@dimen/dp_92"
               android:layout_marginTop="@dimen/dp_8"
               android:background="@drawable/homecard"
               app:layout_constraintEnd_toStartOf="@+id/guideline40"
               app:layout_constraintHorizontal_bias="1.0"
               app:layout_constraintStart_toStartOf="@+id/guideline39"
               app:layout_constraintTop_toBottomOf="@+id/view12" />

           <View
               android:id="@+id/view14"
               android:layout_width="0dp"
               android:layout_height="@dimen/dp_45"
               android:background="@drawable/homecard"
               android:layout_marginTop="@dimen/dp_8"
               app:layout_constraintEnd_toStartOf="@+id/guideline40"
               app:layout_constraintHorizontal_bias="1.0"
               app:layout_constraintStart_toStartOf="@+id/guideline39"
               app:layout_constraintTop_toBottomOf="@+id/view13" />

       </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>